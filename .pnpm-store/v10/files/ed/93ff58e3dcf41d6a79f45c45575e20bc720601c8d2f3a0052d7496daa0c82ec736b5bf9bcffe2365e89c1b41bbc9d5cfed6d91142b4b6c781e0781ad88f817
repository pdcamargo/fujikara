import * as _jsx_runtime from 'react/jsx-runtime';
import { MDXContentProps } from 'mdx-bundler/client/index.js';

declare function useMDXComponent(code: string): React.FunctionComponent<MDXContentProps>;
type Props = MDXContentProps & {
    code: string;
};
declare function MDXContent({ code, ...props }: Props): _jsx_runtime.JSX.Element;

export { MDXContent, useMDXComponent };
